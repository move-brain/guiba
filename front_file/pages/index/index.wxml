    <view bindtap="search" style="background-color:#f1f1f1 ;" class="search-form round">
      <text class="cuIcon-search"></text>
      <view type="text" style="color: #bbb;" confirm-type="search">搜索发现</view>
    </view>
   </view>
   <scroll-view  style=" height:12vmin ;  background-color: #f1f1f1;" scroll-x  class="nav" scroll-with-animation scroll-left="{{scrollLeft}}">
  <view class="cu-item {{index==TabCur?'text-mauve cur':''}}" wx:for="{{list}}" wx:key="index" bindtap="tabSelect" data-id="{{index}}">
    {{item.text}}
  </view>
</scroll-view>
</view>
